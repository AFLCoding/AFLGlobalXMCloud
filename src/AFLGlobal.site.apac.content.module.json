{
  "$schema": "../.sitecore/schemas/ModuleFile.schema.json",
  "namespace": "Content.APAC",
  "items": {
    "includes": [
      {
        "name": "apac/site",
        "path": "/sitecore/content/AFLGlobal/APAC",
        "scope": "ItemAndDescendants",
        "allowedPushOperations": "CreateAndUpdate"
      },
      {
        "name": "migrated/apac/site",
        "path": "/sitecore/content/AFL-Global/APAC",
        "scope": "SingleItem",
        "allowedPushOperations": "CreateOnly"
      },
      {
        "name": "migrated/apac/home",
        "path": "/sitecore/content/AFL-Global/APAC/Home",
        "scope": "SingleItem",
        "allowedPushOperations": "CreateAndUpdate"
      },
      {
        "name": "apac/home/solutions",
        "path": "/sitecore/content/AFL-Global/APAC/Home/Solutions",
        "scope": "ItemAndDescendants",
        "allowedPushOperations": "CreateAndUpdate"
      },
      {
        "name": "apac/home/resources",
        "path": "/sitecore/content/AFL-Global/APAC/Home/Resources",
        "scope": "ItemAndDescendants",
        "allowedPushOperations": "CreateAndUpdate"
      },
      {
        "name": "apac/home/contact",
        "path": "/sitecore/content/AFL-Global/APAC/Home/Contact",
        "scope": "ItemAndDescendants",
        "allowedPushOperations": "CreateAndUpdate"
      },
      {
        "name": "apac/home/company",
        "path": "/sitecore/content/AFL-Global/APAC/Home/Company",
        "scope": "ItemAndDescendants",
        "allowedPushOperations": "CreateAndUpdate"
      },
      {
        "name": "apac/home/404",
        "path": "/sitecore/content/AFL-Global/APAC/Home/404",
        "scope": "ItemAndDescendants",
        "allowedPushOperations": "CreateAndUpdate"
      },
      {
        "name": "apac/home/searchresult",
        "path": "/sitecore/content/AFL-Global/APAC/Home/Search Result",
        "scope": "ItemAndDescendants",
        "allowedPushOperations": "CreateAndUpdate"
      },
      {
        "name": "apac/home/sitemap",
        "path": "/sitecore/content/AFL-Global/APAC/Home/sitemap",
        "scope": "ItemAndDescendants",
        "allowedPushOperations": "CreateAndUpdate"
      },
      {
        "name": "apac/home/overlays",
        "path": "/sitecore/content/AFL-Global/APAC/Home/Overlays",
        "scope": "ItemAndDescendants",
        "allowedPushOperations": "CreateAndUpdate"
      },
      {
        "name": "apac/presentation",
        "path": "/sitecore/content/AFL-Global/APAC/Presentation",
        "scope": "ItemAndDescendants",
        "allowedPushOperations": "CreateAndUpdate"
      },
      {
        "name": "apac/settings",
        "path": "/sitecore/content/AFL-Global/APAC/Settings",
        "rules": [
          {
            "path": "/Creative Exchange Storages",
            "scope": "Ignored"
          },
          {
            "path": "*",
            "scope": "ItemAndDescendants",
            "allowedPushOperations": "CreateAndUpdate"
          }
        ]
      }
    ]
  }
}