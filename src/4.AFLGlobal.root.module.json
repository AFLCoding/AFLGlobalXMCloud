{
  "$schema": "../.sitecore/schemas/ModuleFile.schema.json",
  "namespace": "AFLGlobal.Root",
  "items": {
    "includes": [
      {
        "name": "root",
        "path": "/sitecore/content/AFLGlobal",
        "scope": "ItemAndDescendants",
        "allowedPushOperations": "CreateOnly"
      },
      {
        "name": "renderingHosts",
        "path": "/sitecore/system/Settings/Services/Rendering Hosts",
        "rules": [
          {
						"path": "/Default",
						"scope": "Ignored"
					},
					{
						"path": "*",
						"scope": "ItemAndDescendants",
						"allowedPushOperations": "CreateAndUpdate"
					}
				]
      }
    ]
  }
}