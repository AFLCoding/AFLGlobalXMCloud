{
  "$schema": "../.sitecore/schemas/ModuleFile.schema.json",
  "namespace": "Content.EMEA",
  "items": {
    "includes": [
      {
        "name": "emea/site",
        "path": "/sitecore/content/AFLGlobal/EMEA",
        "scope": "ItemAndDescendants",
        "allowedPushOperations": "CreateAndUpdate"
      },
      {
        "name": "migrated/emea/site",
        "path": "/sitecore/content/AFL-Global/EMEA",
        "scope": "SingleItem",
        "allowedPushOperations": "CreateOnly"
      },
      {
        "name": "migrated/emea/home",
        "path": "/sitecore/content/AFL-Global/EMEA/Home",
        "scope": "SingleItem",
        "allowedPushOperations": "CreateAndUpdate"
      },
      {
        "name": "emea/home/solutions",
        "path": "/sitecore/content/AFL-Global/EMEA/Home/Solutions",
        "scope": "ItemAndDescendants",
        "allowedPushOperations": "CreateAndUpdate"
      },
      {
        "name": "emea/home/resources",
        "path": "/sitecore/content/AFL-Global/EMEA/Home/Resources",
        "scope": "ItemAndDescendants",
        "allowedPushOperations": "CreateAndUpdate"
      },
      {
        "name": "emea/home/contact",
        "path": "/sitecore/content/AFL-Global/EMEA/Home/Contact",
        "scope": "ItemAndDescendants",
        "allowedPushOperations": "CreateAndUpdate"
      },
      {
        "name": "emea/home/company",
        "path": "/sitecore/content/AFL-Global/EMEA/Home/Company",
        "scope": "ItemAndDescendants",
        "allowedPushOperations": "CreateAndUpdate"
      },
      {
        "name": "emea/home/404",
        "path": "/sitecore/content/AFL-Global/EMEA/Home/404",
        "scope": "ItemAndDescendants",
        "allowedPushOperations": "CreateAndUpdate"
      },
      {
        "name": "emea/home/searchresult",
        "path": "/sitecore/content/AFL-Global/EMEA/Home/Search Result",
        "scope": "ItemAndDescendants",
        "allowedPushOperations": "CreateAndUpdate"
      },
      {
        "name": "emea/home/sitemap",
        "path": "/sitecore/content/AFL-Global/EMEA/Home/sitemap",
        "scope": "ItemAndDescendants",
        "allowedPushOperations": "CreateAndUpdate"
      },
      {
        "name": "emea/home/overlays",
        "path": "/sitecore/content/AFL-Global/EMEA/Home/Overlays",
        "scope": "ItemAndDescendants",
        "allowedPushOperations": "CreateAndUpdate"
      },
      {
        "name": "emea/presentation",
        "path": "/sitecore/content/AFL-Global/EMEA/Presentation",
        "scope": "ItemAndDescendants",
        "allowedPushOperations": "CreateAndUpdate"
      },
      {
        "name": "emea/settings",
        "path": "/sitecore/content/AFL-Global/EMEA/Settings",
        "rules": [
          {
            "path": "/Creative Exchange Storages",
            "scope": "Ignored"
          },
          {
            "path": "*",
            "scope": "ItemAndDescendants",
            "allowedPushOperations": "CreateAndUpdate"
          }
        ]
      }
    ]
  }
}