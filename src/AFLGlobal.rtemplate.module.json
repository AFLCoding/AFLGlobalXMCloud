{
	"$schema": "../.sitecore/schemas/ModuleFile.schema.json",
	"namespace": "AFLGlobal.Template",
	"items": {
		"includes": [
			{
				"name": "template/Project",
				"path": "/sitecore/templates/Project/AFLGlobal",
				"rules": [
					{
						"path": "*",
						"scope": "ItemAndDescendants",
						"allowedPushOperations": "CreateAndUpdate"
					}
				]
			},
			{
				"name": "migrated/template/Project",
				"path": "/sitecore/templates/Project/AFL-Global",
				"rules": [
					{
						"path": "*",
						"scope": "ItemAndDescendants",
						"allowedPushOperations": "CreateAndUpdate"
					}
				]
			},
			{
				"name": "migrated/branches/Feature",
				"path": "/sitecore/templates/Branches/Feature/AFL",
				"rules": [
					{
						"path": "*",
						"scope": "ItemAndDescendants",
						"allowedPushOperations": "CreateAndUpdate"
					}
				]
			},
			{
				"name": "migrated/template/feature",
				"path": "/sitecore/templates/Feature",
				"rules": [
					{
						"path": "/AFL",
						"scope": "ItemAndDescendants",
						"allowedPushOperations": "CreateAndUpdate"
					},
					{
						"path": "/AFL Email Templates",
						"scope": "ItemAndDescendants",
						"allowedPushOperations": "CreateAndUpdate"
					},
					{
						"path": "/AFL Page Templates",
						"scope": "ItemAndDescendants",
						"allowedPushOperations": "CreateAndUpdate"
					},
					{
						"path": "*",
						"scope": "Ignored"
					}
				]
			}
		]
	}
}